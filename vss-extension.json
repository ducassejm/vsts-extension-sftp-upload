{
    "manifestVersion": 1,
    "id": "sftpupload",
    "name": "SFTP Upload",
    "version": "1.0.8",
    "publisher": "jean-marc-ducasse",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "galleryFlags": [ "Public" ],
    "description": "Use SFTP to transfer files to a remote server.",
    "categories": [
        "Build and release"
    ],
    "tags": [
        "sftp",
        "file transfer",
        "copy file",
        "SSH"
    ],    
    "icons": {
        "default": "images/sftpupload-extension-icon.png"
    },
    "screenshots": [
        {
            "path": "images/sftpupload-entry-form.png"
        },
        {
            "path": "images/sftpupload-add-task.png"
        }
    ], 
    "content": {
        "details": {
            "path": "overview.md"
        },
        "license": {
            "path": "LICENSE.md"
        }
    },       
    "files": [
        {
            "path": "sftpupload"
        }
    ],
    "links": {
        "getstarted": {
            "uri": "https://github.com/ducassejm/vsts-extension-sftp-upload#getting-started"
        },        
        "support": {
            "uri": "https://github.com/ducassejm/vsts-extension-sftp-upload/issues"
        }
    },    
    "contributions": [
        {
            "id": "sftp-upload",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "sftpupload"
            }
        }
    ]
}